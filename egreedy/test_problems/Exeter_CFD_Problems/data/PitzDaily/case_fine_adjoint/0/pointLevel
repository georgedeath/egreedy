FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    location    "0";
    object      pointLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    upperWall
    {
        type            calculated;
    }
    lowerWall
    {
        type            calculated;
    }
    frontAndBack
    {
        type            empty;
    }
    ribbon
    {
        type            calculated;
    }
}


